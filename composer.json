{
    "name": "aryeo/eloquent-filters",
    "description": "A package to add filters to Eloquent query builder scopes.",
    "keywords": [
        "Aryeo",
        "laravel",
        "eloquent"
    ],
    "homepage": "https://github.com/aryeohq/eloquent-filters",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rob Fonseca",
            "email": "robfo@zillowgroup.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "laravel/framework": "^12.36"
    },
    "require-dev": {
        "aryeo/tooling-laravel": "^0.0.9",
        "brianium/paratest": "^7.14",
        "nikic/php-parser": "^5.6",
        "orchestra/testbench": "^10.6",
        "orchestra/testbench-core": "^10.7",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12",
        "rector/rector": "^2.1"
    },
    "repositories": {
        "tooling-laravel": {
            "type": "vcs",
            "url": "https://github.com/AryeoHQ/tooling-laravel"
        }
    },
    "autoload": {
        "psr-4": {
            "Support\\": "src/Support/",
            "Tooling\\": "src/Tooling/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "aryeo/tooling-laravel": true
        }
    },
    "extra": {
        "tooling": {
            "phpstan": "tooling/phpstan/rules.neon",
            "rector": {
                "rules": "tooling/rector/rules.php"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
